# --- Weaviate Connection ---
# Option 1: Local Weaviate instance
WEAVIATE_HOST="localhost"
WEAVIATE_PORT="8080"

# Option 2: Weaviate Cloud Services (WCS)
# Uncomment and fill these if you are using WCS
# WCS_CLUSTER_URL="YOUR_WCS_CLUSTER_URL"
# WCS_API_KEY="YOUR_WCS_API_KEY"

# --- Neo4j AuraDB Connection Details ---
NEO4J_URI="neo4j+s://your-neo4j-auradb-uri.databases.neo4j.io"
NEO4J_USERNAME="neo4j"
NEO4J_PASSWORD="YOUR_AURA_DB_PASSWORD"

# Google Generative AI
GEMINI_API_KEY="YOUR_GEMINI_API_KEY"
# Default generative model for low-latency reasoning (override as needed)
GEMINI_MODEL_NAME="gemini-2.5-flash-lite-preview-06-17"
# Higher-capacity model for complex agent reasoning
GEMINI_MODEL_NAME_FULL="gemini-2.5-flash"

# --- File Watcher --- 
# Comma-separated list of directories to watch for code changes.
WATCH_PATHS="src"
